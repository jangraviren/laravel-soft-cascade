{
    "name": "jangraviren/laravel-soft-cascade",
    "description": "Laravel Cascade Soft Delete & Restore, based on askedio package",
    "type": "library",
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.8|^6.0",
        "illuminate/container": "^5.8|^6.0",
        "illuminate/database": "^5.8|^6.0",
        "illuminate/events": "^5.8|^6.0"
     },
     "require-dev": {
        "phpunit/phpunit": "~8.0",
        "orchestra/testbench": "~4.0",
        "codacy/coverage": "dev-master"
     },
    "autoload": {
        "psr-4": {
            "JangraViren\\SoftCascade\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JangraViren\\Tests\\" : "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JangraViren\\SoftCascade\\Providers\\GenericServiceProvider"
            ]
        }
    }
}
